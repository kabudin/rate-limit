{
  "name": "zeno/rate-limit",
  "description": "Redis based rate limiter in the hyperf framework",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "rate-limit"
  ],
  "autoload": {
    "psr-4": {
      "Zeno\\RateLimit\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/di": "^3.1",
    "hyperf/redis": "^3.1"
  },
  "suggest": {},
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.1-dev",
      "3.1": "3.1.0"
    },
    "hyperf": {
      "config": "Zeno\\RateLimit\\ConfigProvider"
    }
  }
}
